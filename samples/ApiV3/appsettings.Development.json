{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../Logs/api.log.txt",
          "rollingInterval": "Day",
          "shared": true
        }
      },
      { "Name": "Console" }
    ]
  }
}
